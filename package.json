{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "predev:backend": "bash -c 'lsof -ti tcp:4000 | xargs kill -9 2>/dev/null || true'",
    "dev:backend": "nodemon --watch server.js --watch lib/ server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "db:init": "docker-compose up -d && sleep 10 && node -e \"require('./lib/migrations').migrate().then(console.log)\"",
    "db:migrate": "node -e \"require('./lib/migrations').migrate().then(console.log)\"",
    "db:rollback": "node -e \"require('./lib/migrations').rollback(process.argv[2] || 'previous').then(console.log)\"",
    "db:backup": "node -e \"require('./lib/backup-manager').createFullBackup().then(console.log)\"",
    "db:status": "node -e \"require('./lib/migrations').getStatus().then(console.log)\"",
    "db:health": "node -e \"require('./lib/database').getConnectionStats().then(console.log)\"",
    "monitoring:start": "docker-compose -f monitoring/elk-stack/docker-compose.elk.yml -f monitoring/prometheus/docker-compose.monitoring.yml up -d",
    "monitoring:stop": "docker-compose -f monitoring/elk-stack/docker-compose.elk.yml -f monitoring/prometheus/docker-compose.monitoring.yml down",
    "monitoring:logs": "docker-compose -f monitoring/elk-stack/docker-compose.elk.yml -f monitoring/prometheus/docker-compose.monitoring.yml logs -f",
    "monitoring:health": "node scripts/check-monitoring-health.js"
  },
